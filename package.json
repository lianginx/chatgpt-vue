{
  "name": "vue3-ts-vite-router-tailwindcss",
  "version": "0.0.0",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
